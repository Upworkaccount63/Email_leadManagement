\r\n      editable: true,\r\n    },\r\n    {\r\n      title: 'Crunchbase',\r\n      dataIndex: 'crunchbase',\r\n      width: 150,\r\n      editable: true,\r\n    },\r\n    {\r\n      title: 'ID',\r\n      width: 75,\r\n      dataIndex: 'key',\r\n    },\r\n    {\r\n      title: 'Update',\r\n      key: 'operationEdit',\r\n      fixed: 'right',\r\n      width: 100,\r\n      render: (_, record) => {\r\n        const editable = isEditing(record);\r\n        return editable ? (\r\n          <span>\r\n            <Button\r\n              type='link'\r\n              onClick={() => save(record.key)}\r\n              style={{\r\n                marginRight: 8,\r\n              }}\r\n            >\r\n              Save\r\n            </Button>\r\n              <br/>\r\n           <Popconfirm title=\"Sure to cancel?\" onConfirm={cancel}>\r\n              <Button type='link'>Cancel</Button>\r\n           </Popconfirm>\r\n           </span>\r\n        ) : (\r\n          <Button type='dashed' disabled={editingKey !== ''} onClick={() => edit(record)}>\r\n            Edit\r\n          </Button>\r\n        );\r\n      },\r\n    },\r\n    {\r\n      title: 'Delete',\r\n      key: 'operationDelete',\r\n      fixed: 'right',\r\n      width: 100,\r\n      render: (text, record) => (\r\n        <Space size=\"middle\">\r\n          <Button danger type='dashed' onClick={e => handleDelete(record)}>\r\n            Delete\r\n          </Button>\r\n        </Space>\r\n      )\r\n    },\r\n    // {\r\n    //   title: 'Validate',\r\n    //   key: 'operationValidate',\r\n    //   fixed: 'right',\r\n    //   width: 100,\r\n    //   render: () =>\r\n    //     <Button type=\"dashed\" onClick={handleValidate}>\r\n    //       Validate\r\n    //     </Button>,\r\n    // }\r\n  ];\r\n\r\n  const mergedColumns = withoutColumns.map((col) => {\r\n    if (!col.editable) {\r\n      return col;\r\n    }\r\n\r\n    return {\r\n      ...col,\r\n      onCell: (record) => ({\r\n        record,\r\n        // inputType: col.dataIndex === 'age' ? 'number' : 'text',\r\n        dataIndex: col.dataIndex,\r\n        title: col.title,\r\n        editing: isEditing(record),\r\n      }),\r\n    };\r\n  });\r\n\r\n  const fetchList = async ()=>{\r\n    await axios.get('http://localhost:8000/api/withoutEmail')\r\n    .then(res => {\r\n      setData(res.data)\r\n      console.log(data)\r\n    })\r\n    .catch(error => console.log(\"GET ERROR\", error))\r\n  }\r\n\r\n  useEffect(()=>{\r\n    fetchList()\r\n  }, [])\r\n\r\n  return (\r\n    <div>\r\n      <h1 styles={{textAlign:'center'}}>Targets Without Emails</h1>\r\n    <Form form={form} component={false} name='withoutEmails'>\r\n      <Table\r\n        components={{\r\n          body: {\r\n            cell: EditableCell,\r\n          },\r\n        }}\r\n        bordered\r\n        dataSource={data}\r\n        rowKey={record => record.key}\r\n        columns={mergedColumns}\r\n        rowClassName=\"editable-row\"\r\n        pagination={{\r\n          onChange: cancel,\r\n        }}\r\n        scroll={{ x: 1500 }}\r\n        sticky\r\n        // extensions \r\n        exportable\r\n        searchable\r\n        searchableProps={{\r\n          fuzzySearch: true,\r\n          inputProps: {\r\n            placeholder: \"Search\",\r\n            prefix: <SearchOutlined />,\r\n          },\r\n          fuzzyProps: {\r\n            treshold: 0.0,\r\n            distance: 0,\r\n            isCaseSensitive: false\r\n          }\r\n        }}\r\n      />\r\n    </Form>\r\n    </div>\r\n  );\r\n};\r\n\r\n\r\nexport default WithoutEmails","C:\\Users\\johnm\\Desktop\\code\\leadManagement\\frontend\\gui\\src\\components\\layout\\HeaderComponents.js",[],"C:\\Users\\johnm\\Desktop\\code\\leadManagement\\frontend\\gui\\src\\components\\emailStage\\EditableCell.js",[],"C:\\Users\\johnm\\Desktop\\code\\leadManagement\\frontend\\gui\\src\\components\\emailStage\\DummyData.js",[],"C:\\Users\\johnm\\Desktop\\code\\leadManagement\\frontend\\gui\\src\\comp